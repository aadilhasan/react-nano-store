{
  "name": "react-nano-store",
  "version": "0.1.0",
  "author": "Aadil Hasan",
  "license": "MIT",
  "description": "react-nano-store is an incredibly lightweight 0.5kb, blazing fast, easy to use state management utility for React.",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "dependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "json": "^11.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.2.0",
    "vitest": "^0.30.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/react-nano-store.umd.js",
  "module": "./dist/react-nano-store.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-nano-store.es.js",
      "require": "./dist/react-nano-store.umd.js"
    }
  },
  "keywords": [
    "react-nano-store",
    "useStore"
  ]
}
